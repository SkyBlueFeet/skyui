{
    "name": "custom-vue-ssr",
    "version": "1.0.0",
    "description": "本项目是基于[vue-hackernews-2.0](https://github.com/vuejs/vue-hackernews-2.0)基础上的自定义修改。将webpack升级到了4.x版本。并提供了Koa2版本的服务端版本。",
    "main": "server.js",
    "scripts": {
        "dev": "export PORT=7070 NODE_ENV=development && node server.js",
        "build:dev": "rimraf dist && npm run build:client:dev && npm run build:server:dev",
        "build:client:dev": "export NODE_ENV=development && webpack --config build/webpack.client.config.js --progress --hide-modules",
        "build:server:dev": "export NODE_ENV=development && webpack --config build/webpack.server.config.js --progress --hide-modules",
        "build:prod": "rimraf dist && npm run build:client:prod && npm run build:server:prod",
        "build:client:prod": "export NODE_ENV=production && webpack --config build/webpack.client.config.js --progress --hide-modules",
        "build:server:prod": "export NODE_ENV=production && webpack --config build/webpack.server.config.js --progress --hide-modules",
        "start": "export PORT=7070 NODE_ENV=production && node server.js",
        "line": ""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BengBu-YueZhang/custom-vue-ssr.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/BengBu-YueZhang/custom-vue-ssr/issues"
    },
    "homepage": "https://github.com/BengBu-YueZhang/custom-vue-ssr#readme",
    "dependencies": {
        "axios": "^0.19.0",
        "express": "^4.17.1",
        "lru-cache": "^5.1.1",
        "postcss-import": "^12.0.1",
        "postcss-url": "^8.0.0",
        "vue": "^2.6.10",
        "vue-router": "^3.1.2",
        "vue-server-renderer": "^2.6.10",
        "vuex": "^3.1.1",
        "vuex-router-sync": "^5.0.0"
    },
    "devDependencies": {
        "mini-css-extract-plugin": "^0.8.0",
        "vue-template-compiler": "^2.6.10"
    }
}
