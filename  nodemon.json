{
    "restartable": "rs",
    "ignore": [".git", ".svn", "node_modules/**/node_modules", "src", "script"],
    "verbose": true,
    "execMap": {
        "js": "ts-node"
    },
    "watch": ["frame"],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js json css js scss ts tsx html ejs"
}
