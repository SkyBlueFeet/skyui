{
  "name": "skyui",
  "version": "1.0.0",
  "description": "一个基于Vue@2的轻量级组件库",
  "main": "dist/skyui.cjs.js",
  "module": "dist/skyui.esm.js",
  "unpkg": "dist/skyui.min.js",
  "keywords": [
    "vue",
    "vuejs",
    "skyui",
    "typescript",
    "rollup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkyBlueFeet/skyui.git"
  },
  "homepage": "https://github.com/SkyBlueFeet/skyui#readme",
  "bugs": {
    "url": "https://github.com/SkyBlueFeet/skyui/issues"
  },
  "author": "skybluefeet <yakun1593@163.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "start": "yarn run clean && cross-env NODE_ENV=development rollup -w -c scripts/rollup.config.dev.js",
    "build": "yarn run clean && cross-env NODE_ENV=production rollup -c scripts/rollup.config.prod.js",
    "test": "jest",
    "serve": "serve -p 8080"
  },
  "files": [
    "src",
    "dist/*.js"
  ],
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "jest": "^22.0.4",
    "prettier": "^1.9.2",
    "rollup": "^1.30.1",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "standard-version": "^4.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5",
    "uglify-es": "^3.3.4"
  }
}
