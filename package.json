    "scripts": {
        "start": "npm run dev",
        "app:dev": "ts-node --project tsconfig.node.json ./script/dev",
        "docs:dev": "vuepress dev docs",
        "build": "ts-node --project tsconfig.node.json script/build.ts && vuepress build docs",
        "lint": "eslint --ext .js,.vue,.ts,.tsx src",
        "analyzer": "npm run build --report",
        "format": "prettier --write \"*/**/*.ts\" \"*/**/*.vue\" \"*/**/*.tsx\" \"*/**/*.js\" \"*/**/*.jsx\" \"*/**/*.json\"",
        "nest:build": "nest build",
        "nest:start": "nest start",
        "nest:dev": "nest start --watch",
        "nest:debug": "nest start --debug --watch",
        "nest:prod": "node dist/main",
        "release": "standard-version",
        "cz": "git add . && git-cz",
        "rollup:build": "ts-node --project tsconfig.node.json ./script/rollup.ts"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -e $GIT_PARAMS"
        }
    },
    "lint-staged": {
        "src/**/*.js": [
            "prettier --write",
            "git add"
        ]
